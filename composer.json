{
    "name": "xabbuh/panda-bundle",
    "description": "integration of the Panda encoding services into the Symfony2 Framework",
    "keywords": ["Panda", "video encoding", "REST", "client", "Symfony", "bundle"],
    "homepage": "https://github.com/xabbuh/PandaBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Flothmann",
            "homepage": "https://github.com/xabbuh"
        }
    ],
    "autoload": {
        "psr-0": { "Xabbuh\\PandaBundle": "" }
    },
    "require": {
        "php": ">=5.3.0",
        "doctrine/common": "~2.0",
        "symfony/config": "~2.2",
        "symfony/console": "~2.3",
        "symfony/dependency-injection": "~2.2",
        "symfony/event-dispatcher": "~2.2",
        "symfony/form": "~2.2",
        "symfony/http-foundation": "~2.2",
        "symfony/http-kernel": "~2.2",
        "symfony/options-resolver": "~2.2",
        "symfony/routing": "~2.2",
        "xabbuh/panda-client": "^1.2.3"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "~0.7",
        "symfony/browser-kit": "~2.2",
        "symfony/finder": "~2.2",
        "symfony/framework-bundle": "~2.2",
        "symfony/validator": "~2.3"
    },
    "target-dir": "Xabbuh/PandaBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
